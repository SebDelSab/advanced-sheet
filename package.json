{
  "name": "advanced-sheet-handler",
  "main": "dist/advanced-sheet-handler.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "private": false,
  "version": "0.1.0",
  "description": "prototype for advanced-sheet-handler",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build --prerender",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "@stencil/core": "^0.6.1",
    "@stencil/router": "latest",
    "ngl": "^2.0.0-dev.28"
  },
  "devDependencies": {
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SebDelSab/advanced-sheet.git"
  },
  "author": "sdelolme",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SebDelSab/advanced-sheet"
  },
  "keywords": [
    "javascript",
    "Stencil",
    "web-components",
    "ngl",
    "molecular structure"
  ],
  "homepage": "https://github.com/SebDelSab/advanced-sheet",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
